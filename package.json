{
  "name": "web-proxy-pool",
  "version": "0.0.6",
  "description": "http proxy pool",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "npm run rebuild",
    "build": "tsc -p .",
    "rebuild": "npm run clean && npm run lint && npm run build",
    "test": "npm run prepare && set DEBUG=proxy-pool && mocha --reporter list",
    "clean": "rm -rf dist",
    "lint": "tslint --project tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:lawrence-peng/proxy-pool.git"
  },
  "keywords": [
    "http proxy",
    "proxy",
    "pool",
    "typescript",
    "ip"
  ],
  "author": "lawrence",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^4.1.1",
    "https-proxy-agent": "^2.2.1",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/node": "^11.12.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.3",
    "tslib": "^1.9.0",
    "tslint": "^5.14.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
